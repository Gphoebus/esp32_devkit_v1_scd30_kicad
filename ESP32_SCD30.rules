
(rules PCB ESP32_SCD30
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 99)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 500.0)
    (clear 300.2)
    (clear 250.0 (type smd_to_turn_gap))
    (clear 75.0 (type smd_smd))
  )
  (padstack "Via[0-1]_1200:700_um"
    (shape
      (circle F.Cu 1200.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1200.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1200:700_um" "Via[0-1]_1200:700_um" default
  )
  (via 
    "Via[0-1]_1200:700_um-kicad_default" "Via[0-1]_1200:700_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1200:700_um-power" "Via[0-1]_1200:700_um" power
  )
  (via_rule
    default "Via[0-1]_1200:700_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1200:700_um-kicad_default"
  )
  (via_rule
    power "Via[0-1]_1200:700_um-power"
  )
  (class default
    /SDA /SCL "unconnected-(U1-Pad3)" "unconnected-(U1-Pad4)" "unconnected-(U1-Pad5)" "unconnected-(U1-Pad6)" "unconnected-(U1-Pad7)" "unconnected-(U1-Pad8)"
    "unconnected-(U1-Pad9)" "unconnected-(U1-Pad10)" "unconnected-(U1-Pad12)" "unconnected-(U1-Pad13)" "unconnected-(U1-Pad15)" "unconnected-(U1-Pad25)" "unconnected-(U1-Pad24)" "unconnected-(U1-Pad23)"
    "unconnected-(U1-Pad21)" "unconnected-(U1-Pad19)" "unconnected-(U1-Pad18)" "unconnected-(U1-Pad17)" "unconnected-(U1-Pad16)" "unconnected-(U2-Pad5)" "unconnected-(U2-Pad6)" "unconnected-(U2-Pad7)"
    "Net-(D1-Pad1)" "Net-(D1-Pad3)" "Net-(D1-Pad4)" /D12 /D13 /D14 "unconnected-(S1-Pad1)" "Net-(S1-Pad3)"
    "unconnected-(U1-Pad22)" "/D35_adc"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class power
    GND 3v3 +8V
    (clearance_class power)
    (via_rule power)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)